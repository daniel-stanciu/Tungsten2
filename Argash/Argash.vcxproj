	if "%%B"=="VERSION_MINOR_NUMBER" (
		echo READ:  %%A %%B            %%C
		echo WRITE: %%A %%B            %%C
		echo.
		echo %%A %%B            %%C&gt;&gt;"%filename%.temp"  
		)	
	if "%%B"=="VERSION_MAJOR_NUMBER" (
		echo READ:  %%A %%B            %%C
		echo WRITE: %%A %%B            %%C
		echo.
		echo %%A %%B            %%C&gt;&gt;"%filename%.temp"  
		)
	if "%%B"=="VERSION_RELEASE_NUMBER" (
		echo READ:  %%A %%B            %%C
		echo WRITE: %%A %%B            %%C
		echo.
		echo %%A %%B            %%C&gt;&gt;"%filename%.temp"  
		)	
	)
del "%filename%"
ren "%filename%.temp" "%filename%"</Command>
