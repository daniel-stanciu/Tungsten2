setlocal enabledelayedexpansion
set filename=version.h
if exist "%filename%.temp" del "%filename%.temp"
for /f "tokens=1-3 delims= " %%A in (version.h) do (
	if "%%B"=="VERSION_BUILD_NUMBER" (
